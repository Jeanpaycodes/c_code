///指针题目四
#include <stdio.h>

int main()
{
   int a[3][2] = {(0,1),(2,3),(4,5)};    ///注意这里是一个逗号表达式 只取最后一个表达式为输出结果
   int *p;                               ///即 1 3
                                         ///   5 0
                                         ///   0 0
   p = a[0];
   printf("%d",p[0]);    ///所以第一个元素即为1
   return 0;
}
